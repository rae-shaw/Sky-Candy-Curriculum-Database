{
  "version": 2,
  "alias": "sky-candy-database",
  "name": "sky-candy-database",
  "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
           "src": "^/favicon.ico$",
           "dest": "/favicon.ico"
        },
        {
           "src": "^/manifest.json$",
           "dest": "/manifest.json"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}